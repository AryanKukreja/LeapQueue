<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.twiliohackathon.leapqueue">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectedStoreActivity"></activity>
        <activity android:name=".ListResultsActivity"></activity>
        <activity android:name=".LoggedInActivity"></activity>
        <activity android:name=".SearchQueryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EmailVerificationActivity"></activity>
        <activity android:name=".ForgotPasswordActivity"></activity>
        <activity android:name=".WelcomePageActivity">
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>